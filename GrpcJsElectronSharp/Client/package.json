{
  "name": "Client",
  "version": "1.0.0",
  "description": "Client",
  "main": "main.js",
  "author": "Skalware",
  "license": "ISC",
  "build": {
    "appId": "client.UI",
    "npmRebuild": false,
    "win": {
      "target": [
        "dir"
      ]
    },
    "compression": "normal",
    "files": [
      "!**/*.{ts,c,h,md,cmd,csproj,user,scss}",
      "!{bin,obj}"
    ],
    "electronDownload": {
      "arch": "ia32"
    },
    "directories": {
      "output": "."
    },
    "extraFiles": [
      {
        "from": "../protos",
        "to": "resources/protos",
        "filter": "*.proto"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "postinstall": "npm rebuild --target=5.0.1 --runtime=electron --dist-url=https://atom.io/download/electron"
  },
  "devDependencies": {
    "@types/node": "14.14.31",
    "jsdoc": "^3.6.2",
    "tmp": "0.2.1",
    "uglify-js": "^3.5.12",
    "espree": "^3.5.4",
    "escodegen": "^1.11.1",
    "estraverse": "^4.2.0",
    "devtron": "^1.4.0",
    "semver": "^5.5.0",
    "electron": "15.1.1",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-builder": "22.10.5"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.6.5",
    "asar": "^3.1.0",
    "@grpc/grpc-js": "1.3.7",
    "minimist": "^1.2.0"
  }
}
